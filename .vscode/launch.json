    {
      "name": "Launch Worker Node",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/src",
      "args": ["--config", "./configs/worker-config.yaml"],
      "cwd": "${workspaceFolder}"
    }
  ]
}
